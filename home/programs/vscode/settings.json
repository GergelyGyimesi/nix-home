{
  "security.workspace.trust.enabled": false,
  "[haskell]": {
    "editor.fontLigatures": "'ss03'"
  },
  "breadcrumbs.enabled": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.detectIndentation": false,
  "editor.fontFamily": "Fira Code",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.formatOnSave": true,
  "editor.minimap.enabled": false,
  "editor.renderControlCharacters": false,
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {
    "[Default Dark+]": {
      "comments": "#aeb3a4d0"
    }
  },
  "editor.wordWrapColumn": 120,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.autoUpdate": false,
  "files.autoSave": "afterDelay",
  "gitlens.codeLens.enabled": false,
  "gitlens.views.compare.location": "gitlens",
  "gitlens.views.fileHistory.location": "gitlens",
  "gitlens.views.lineHistory.location": "gitlens",
  "gitlens.views.repositories.location": "gitlens",
  "gitlens.views.search.location": "gitlens",
  "haskell.diagnosticsOnChange": true,
  "haskell.formatOnImportOn": true,
  "haskell.formattingProvider": "ormolu",
  "haskell.languageServerVariant": "haskell-language-server",
  "haskell.liquidOn": false,
  "haskell.logFile": "",
  "haskell.maxNumberOfProblems": 100,
  "haskell.serverExecutablePath": "haskell-language-server",
  "haskell.trace.server": "off",
  "haskell.updateBehavior": "prompt",
  "haskell.indentationRules.enabled": true,
  "html.format.endWithNewline": true,
  "nix.enableLanguageServer": true,
  "nix.formatterPath": "nixpkgs-fmt",
  "nix.serverPath": "rnix-lsp",
  "plantuml.exportFormat": "svg",
  "plantuml.java": "/usr/bin/java",
  "redhat.telemetry.enabled": false,
  "search.showLineNumbers": true,
  "search.smartCase": true,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.allowChords": false,
  "terminal.integrated.ignoreBracketedPasteMode": true,
  "terminal.integrated.copyOnSelection": true,
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontFamily": "Fira Code",
  "terminal.integrated.macOptionIsMeta": true,
  "terminal.integrated.scrollback": 100000,
  "terminal.integrated.sendKeybindingsToShell": false,
  "terminal.integrated.inheritEnv": false,
  "terminal.integrated.tabs.hideCondition": "singleGroup",
  "terminal.integrated.defaultProfile.osx": "fish",
  "terminal.integrated.profiles.osx": {
    "fish": {
      "path": "/Users/pwm/.nix-profile/bin/fish",
      "args": ["-l"]
    }
  },
  "update.mode": "none",
  "window.restoreWindows": "all",
  "window.zoomLevel": 1,
  "workbench.colorCustomizations": {
    "editorError.foreground": "#ff0000",
    "editorError.border": "#ff0000",
    "editorHint.foreground": "#ffa500",
    "editorHint.border": "#ffa500",
    "editorInfo.foreground": "#ffa500",
    "editorInfo.border": "#ffa500",
    "editorWarning.foreground": "#ff4500",
    "editorWarning.border": "#ff4500"
  },
  "workbench.editorAssociations": {
    "*.hex": "hexEditor.hexedit"
  },
  "workbench.editor.enablePreview": false,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.panel.defaultLocation": "right",
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.startupEditor": "none",
  "yaml.format.enable": true,
  "yaml.format.printWidth": 160
}
